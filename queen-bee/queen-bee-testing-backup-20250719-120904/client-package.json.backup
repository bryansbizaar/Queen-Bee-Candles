{
  "name": "queen-bee",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 3000",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:components": "vitest run src/components",
    "test:pages": "vitest run src/pages",
    "test:api": "vitest run src/tests/api",
    "test:cart": "vitest run src/tests/workflows/Cart*.test.jsx",
    "test:cart-watch": "vitest src/tests/workflows/Cart*.test.jsx",
    "test:workflows": "vitest run src/tests/workflows/",
    "test:integration": "vitest run src/tests/workflows/",
    "test:debug": "vitest run --reporter=verbose --no-coverage",
    "test:silent": "vitest run --silent",
    "test:changed": "vitest run --changed",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:headed": "playwright test --headed",
    "test:all": "npm test && npm run e2e"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "lucide-react": "^0.517.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.5.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "resize-observer-polyfill": "^1.5.1",
    "vite": "^5.3.1",
    "vitest": "^3.2.4"
  }
}
